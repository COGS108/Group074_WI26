import pandas as pd
import seaborn as sns
import numpy as np

salary_df_raw = pd.read_csv("data/00-raw/salary_data_raw.csv")
salary_df_current = salary_df_raw.drop(["2026-27", "2027-28","2028-29"], axis = 1)

salary_df = pd.melt(salary_df_current, id_vars=['name'], var_name='Season', value_name='Salary').dropna()

salary_df =salary_df.assign(Two_Way = salary_df.Salary.apply(lambda sal: sal[0:2] == "TW"))
salary_df = salary_df.assign(Salary = salary_df.Salary.apply(lambda sal: int(''.join([c for c in sal if c.isnumeric()]))))
salary_df


